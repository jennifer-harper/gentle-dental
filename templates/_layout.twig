<!DOCTYPE html>
<html lang="{{ craft.app.language }}">
<head>
    {% block head %}
      <meta charset="utf-8"/>
      <title>{{ docTitle ?? siteName }}</title>
      <meta content="width=device-width, initial-scale=1.0" name="viewport">
      <meta name="description" content="{{ metaDesc|default('Gentle Dental meta') }}">
      <link rel="icon" href="/uploads/images/gd-favicon.svg" type="image/x-icon" />
      <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/swiper@8/swiper-bundle.min.css"/>
      {% do craft.app.view.registerCssFile('@web/assets/styles/styles.css') %}
    {% endblock %}
</head>
{% include '_partials/nav.twig' %}

  
<body {{ attr(bodyAttributes ?? {}) }}>
  {% block content %}{% endblock %}
  {% include '_partials/footer.twig' %}
  {% block scripts %}
    <script src="https://code.jquery.com/jquery-3.6.4.min.js"></script>
    <script src="https://cdn.jsdelivr.net/npm/swiper@8/swiper-bundle.min.js"></script>
    <script  src="/assets/js/script.js"></script>
    <script  type="module" src="/assets/js/slider.js"></script>
  {% endblock %}
</body>
</html>


