    {% set image = entry.styleSixImage.all() %}
    <div class="dark flex-wrap bg-700 style-six">
      <div class="pad-all pad-lg align-items-center text-center">
        {% if entry.styleSixTitle %}
          <h2>{{entry.styleSixTitle}}</h2>
        {% endif %}

        <div class="pad-top pad-btm flex-wrap just-cont-center-row">

          <div class="third {% block styleOne %}  {% endblock %}">
            {% if image|length >= 2 %}
            <img src="{{ image[0].getUrl() }}" alt="{{ image[0].alt }}">
            {% endif %}
          </div>

          <div class="third text-style-one just-cont-center">
            <div class="intro">
              {% if entry.styleSixTextBold %}
                {{entry.styleSixTextBold}}
              {% endif %}
            </div>
              {% if entry.styleSixText %}
                {{entry.styleSixText}}
              {% endif %}
          </div>

          <div class="third {% block styleTwo %}  {% endblock %}">
            <div>
              {% if image|length >= 2 %}
                <img src="{{ image[1].getUrl() }}" alt="{{ image[1].alt }}">
              {% endif %}
            </div>
            <div>
              {% if image|length >= 2 %}
                <img src="{{ image[2].getUrl() }}" alt="{{ image[2].alt }}">
              {% endif %}
            </div>
          </div>
        </div>
      </div>
    </div>